<thlogic>
    <attr sel="#headId" th:replace="~{fragments/head :: headFragment}" />
    <attr sel="#game-over" th:text="#{game.over}" />
    <attr sel="#result-message" th:text="${__${T(igor.learnprogramming.util.AttributeNames).RESULT_MESSAGE}__}" />
    <attr sel="#restart"
          th:href="@{__${T(igor.learnprogramming.util.GameMappings).RESTART}__}"
          th:text="#{game.restart}"
    />
    <attr sel="#home"
          th:href="@{__${T(igor.learnprogramming.util.GameMappings).HOME}__}"
          th:text="#{game.home}"
    />
    <attr sel="#footerId" th:replace="~{fragments/footer :: footFragment}" />
</thlogic>
